{
	"name": "structure-bytes",
	"version": "2.13.9",
	"description": "Efficiently stores data of any shape by separating it into structure and value",
	"main": "index.js",
	"dependencies": {
		"base64-js": "1.1.x",
		"sha256": "0.2.x"
	},
	"devDependencies": {
		"babel-cli": "6.11.x",
		"babel-plugin-istanbul": "2.0.x",
		"babel-preset-es2015": "6.13.x",
		"babelify": "7.3.x",
		"browserify": "13.1.x",
		"coveralls": "2.11.x",
		"cross-env": "2.0.x",
		"nyc": "8.1.x",
		"uglify-js": "2.7.x"
	},
	"directories": {
		"test": "test"
	},
	"scripts": {
