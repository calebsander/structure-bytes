{
	"name": "structure-bytes",
	"version": "3.2.11",
	"description": "A library for more efficient data transfers by separating the structure from the values and storing each as binary data",
	"main": "dist/index.js",
	"dependencies": {
		"accepts": "^1.3.3",
		"base64-js": "^1.2.0",
		"js-sha256": "^0.6.0"
	},
	"devDependencies": {
		"@types/base64-js": "^1.2.5",
		"@types/node": "^8.0.20",
		"babel-cli": "^6.24.1",
		"babel-plugin-istanbul": "^4.1.1",
		"babel-preset-es2015": "^6.24.1",
		"browserify": "^14.3.0",
		"coveralls": "^2.13.0",
		"google-closure-compiler": "^20170409.0.0",
		"nyc": "^10.3.2",
		"tslint": "^5.6.0",
		"typedoc": "^0.8.0",
		"typescript": "^2.4.2"
	},
	"directories": {
		"test": "test"
	},
	"scripts": {
		"build": "rm -rf dist && tsc",
		"docs": "rm -rf docs && typedoc --out docs --name structure-bytes --readme README.md",
		"test": "npm run build && tsc -p test/tsconfig.json && node test/main.js no-compile && npm run tslint",
		"test-travis": "nyc --reporter=lcov npm test && coveralls < coverage/lcov.info",
		"tslint": "tslint -p . -c tslint.json -t verbose"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/calebsander/structure-bytes.git"
	},
	"keywords": [
		"data",
		"structure",
		"bytes",
		"file",
		"transfer",
		"type",
		"value"
	],
	"author": "Caleb Sander",
	"license": "MIT",
	"engines": {
		"node": ">=8.0.0"
	},
	"bugs": {
		"url": "https://github.com/calebsander/structure-bytes/issues"
	},
	"homepage": "https://github.com/calebsander/structure-bytes"
}
